#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/sanity@3.99.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.27_@types+react-dom@19.2.3_@type_k3crfz2m7gncnyptujfdt6mafy/node_modules/sanity/bin/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/sanity@3.99.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.27_@types+react-dom@19.2.3_@type_k3crfz2m7gncnyptujfdt6mafy/node_modules/sanity/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/sanity@3.99.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.27_@types+react-dom@19.2.3_@type_k3crfz2m7gncnyptujfdt6mafy/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/sanity@3.99.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.27_@types+react-dom@19.2.3_@type_k3crfz2m7gncnyptujfdt6mafy/node_modules/sanity/bin/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/sanity@3.99.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.27_@types+react-dom@19.2.3_@type_k3crfz2m7gncnyptujfdt6mafy/node_modules/sanity/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/sanity@3.99.0_@emotion+is-prop-valid@1.2.2_@types+node@20.19.27_@types+react-dom@19.2.3_@type_k3crfz2m7gncnyptujfdt6mafy/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sanity/bin/sanity" "$@"
else
  exec node  "$basedir/../sanity/bin/sanity" "$@"
fi
