#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.2.7/node_modules/@sanity/import/src/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.2.7/node_modules/@sanity/import/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.2.7/node_modules/@sanity/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.2.7/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.2.7/node_modules/@sanity/import/src/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.2.7/node_modules/@sanity/import/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.2.7/node_modules/@sanity/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/@sanity+import@3.38.3_@types+react@19.2.7/node_modules:/mnt/c/Users/Frikk/Documents/GitHub/flyt-next/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@sanity+import@3.38.3_@types+react@19.2.7/node_modules/@sanity/import/src/cli.js" "$@"
else
  exec node  "$basedir/../../../../../@sanity+import@3.38.3_@types+react@19.2.7/node_modules/@sanity/import/src/cli.js" "$@"
fi
